  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "browserslist/browserslist",
  "dependencies": {
    "caniuse-lite": "^1.0.30001021",
    "electron-to-chromium": "^1.3.338",
    "node-releases": "^1.1.46"
  },
  "bin": "./cli.js",
  "browser": {
